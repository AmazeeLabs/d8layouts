{%
  set classes = [
    'node',
    'node--type-' ~ node.bundle|clean_class,
    node.isPromoted() ? 'node--promoted',
    node.isSticky() ? 'node--sticky',
    not node.isPublished() ? 'node--unpublished',
    view_mode ? 'node--view-mode-' ~ view_mode|clean_class,
    'clearfix',
  ]
%}
asdf
<article{{ attributes.addClass(classes) }}>
  <header>
    {{ title_prefix }}
    {% if not page %}
      <h2{{ title_attributes.addClass('node__title') }}>
        <a href="{{ url }}" rel="bookmark">{{ label }}</a>
      </h2>
    {% endif %}
    {{ title_suffix }}
  </header>

  <div class="cool_themers__cool_pic">
    {{ content.field_cool_pic }}
  </div>

  <div class="cool_themers__cool_attributes">
    {{ content.field_cool_attributes }}
  </div>

  <div{{ content_attributes.addClass('node__content') }}>
    {{ content|without('links', 'field_cool_attributes', 'field_cool_pic') }}
  </div>

  {% if content.links %}
    <div class="node__links">{{ content.links }}</div>
  {% endif %}
</article>
